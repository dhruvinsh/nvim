{
  "keymap": {
    "prefix": "kmap",
    "body": [
      "[\"${1:key}\"] = { ${2:action}, \"${3:description}\" }"
    ],
    "description": "Keymap"
  },
  "keymapopts": {
    "prefix": "kmapopt",
    "body": [
      "[\"${1:key}\"] = { ${2:action}, \"${3:description}\", { ${4:opts} } }"
    ],
    "description": "Keymap with opts"
  }
}
